{
  "name": "lazycss",
  "version": "0.0.5",
  "description": "Javascript powered css",
  "main": "src/lazycss.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "beautify": "uglifyjs src/lazycss.js -o dist/lazycss.js -b space-colon=true,semicolons=false,quote_style=1,indent-level=4",
    "minify": "uglifyjs src/lazycss.js -o dist/lazycss.min.js -c dead_code,unused,booleans,loops,join_vars,drop_debugger,drop_console,sequences -m",
    "prepublish": "npm run beautify && npm run minify"
  },
  "keywords": [
    "lazycss",
    "js2css",
    "jscss",
    "j2c"
  ],
  "author": "bigopon <bigopon.777@gmail.com> (http://bigopon.surge.sh)",
  "license": "MIT",
  "repository": {
    "url": "bigopon/lazycss",
    "type": "git"
  }
}
